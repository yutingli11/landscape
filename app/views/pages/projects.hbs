<div id="houseLayout" class="container h-100">
    <div class="d-flex h-100 text-center align-items-center">
        <div class="w-100 text-black">
            <h1 class="display-5">标准户型</h1>
        </div>
    </div>
    <div id="houseLayoutHolder"></div>
    <script id="houseLayoutHolder-template" type="text/x-handlebars-template">
        \{{#each houseLayoutObjs}}
                <div class="row">
                    \{{#each p}}
                    <!-- Team Member 1 -->
                    <div class="col-xl-4 col-md-6 col-sm-1 col-xs-6 mb-4">
                        <div class="card border-0 shadow pop">
                            <img src="\{{img}}"
                                class="card-img-top" alt="...">
                            <div class="card-body text-center">
                                <h5 class="card-title mb-0">\{{txtHeader}}</h5>
                                <div class="card-text text-black-50">\{{txtContent}}</div>
                            </div>
                        </div>
                    </div>
                 \{{/each}}
                </div>
        \{{/each}}
    </script>
</div>

